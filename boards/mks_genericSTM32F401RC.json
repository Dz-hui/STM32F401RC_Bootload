{
    "build": {
      "core": "stm32",
      "cpu": "cortex-m4",
      "extra_flags": "-DSTM32F401xC -DSTM32F4xx",
      "f_cpu": "84000000L",
      "framework_extra_flags": {
        "arduino": "-D__CORTEX_SC=0"
      },
      "mcu": "stm32f401rct6",
      "product_line": "STM32F401xC",
      "variant": "STM32F4xx/F401R(B-C-D-E)T"
    },
    "debug": {
      "default_tools": [
        "jlink"
      ],
      "jlink_device": "STM32F401RC",
      "onboard_tools": [
        "stlink"
      ],
      "openocd_target": "stm32f4x",
      "svd_path": "STM32F401x.svd"
    },
    "frameworks": [
      "arduino",
      "cmsis",
      "libopencm3",
      "stm32cube",
      "zephyr"
    ],
    "name": "STM32F401RC (64k RAM. 256k Flash)",
    "upload": {
      "maximum_ram_size": 65536,
      "maximum_size": 262144,
      "protocol": "jlink",
      "protocols": [
        "stlink",
        "jlink",
        "cmsis-dap",
        "blackmagic",
        "mbed"
      ]
    },
    "url": "https://www.st.com/en/microcontrollers-microprocessors/stm32f401rc.html",
    "vendor": "Generic"
  }
  